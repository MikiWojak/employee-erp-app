{
    "name": "backend-node",
    "version": "1.0.0",
    "main": "index.js",
    "private": true,
    "author": "Mikołaj Żarnowski",
    "license": "ISC",
    "engines": {
        "npm": "^10.0.0",
        "node": "^22.0.0"
    },
    "scripts": {
        "test": "NODE_ENV=test jest --detectOpenHandles --verbose",
        "db-setup-test": "NODE_ENV=test npm run db-drop && NODE_ENV=test npm run db-create && NODE_ENV=test npm run db-migrate",
        "db-undo-migrate": "./node_modules/.bin/sequelize db:migrate:undo:all",
        "db-migrate": "./node_modules/.bin/sequelize db:migrate",
        "db-seed": "./node_modules/.bin/sequelize db:seed --seed 20210623070053-roles.js 20210623070055-departments.js 20210623070056-users.js 20230530174310-feedback-questions.js",
        "db-drop": "./node_modules/.bin/sequelize db:drop",
        "db-create": "./node_modules/.bin/sequelize db:create",
        "db-setup": "npm run db-undo-migrate && npm run db-migrate && npm run db-seed",
        "db-setup-fresh": "npm run redis-session-flush && npm run db-drop && npm run db-create && npm run db-migrate && npm run db-seed",
        "dev-db-seed": "./node_modules/.bin/sequelize db:seed:all",
        "dev-db-setup-fresh": "npm run redis-session-flush && npm run db-drop && npm run db-create && npm run db-migrate && npm run dev-db-seed",
        "dev": "nodemon ./src/index.js",
        "queues": "nodemon ./src/queues.js",
        "redis-session-flush": "docker compose exec redis_session redis-cli -a pass FLUSHALL"
    },
    "dependencies": {
        "amqplib": "0.10.5",
        "bcrypt": "5.1.1",
        "body-parser": "1.20.3",
        "connect-redis": "6.0.0",
        "cors": "2.8.5",
        "dayjs": "1.11.13",
        "deepmerge": "4.3.1",
        "dotenv": "16.4.7",
        "express": "4.21.2",
        "express-handlebars": "8.0.1",
        "express-session": "1.18.1",
        "express-validator": "7.2.1",
        "http-status-codes": "2.3.0",
        "is-plain-object": "5.0.0",
        "multer": "1.4.5-lts.2",
        "mysql2": "3.14.0",
        "node-dependency-injection": "2.7.3",
        "nodemailer": "6.10.0",
        "nodemailer-express-handlebars": "7.0.0",
        "nodemon": "3.1.9",
        "redis": "3.1.2",
        "sequelize": "6.37.6",
        "sequelize-cli": "6.6.2"
    },
    "devDependencies": {
        "eslint": "9.29.0",
        "faker": "5.5.3",
        "jest": "29.7.0",
        "prettier": "3.5.3",
        "supertest": "7.0.0",
        "supertest-session": "5.0.1"
    }
}
